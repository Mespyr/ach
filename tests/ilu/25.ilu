# print argv and length
@include "std/io.ilu"
@include "std/string.ilu"

def main
    argv argc
    while dup 0 > do
        over read64     # get value in argv
        dup strlen swp  # get length of value
        over 
        swp puts 
        " " puts
        dump
        1 -             # decrement argc
        swp 8 + swp     # point to next value in argv
    end
end
